d":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"GuideParentable"}],"url":"\/documentation\/c3\/guideparentable"},"doc://com.davidbjames.C3/documentation/C3/Alert/init()":{"role":"symbol","title":"init()","fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"()"}],"abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Alert\/init()","kind":"symbol","type":"topic","url":"\/documentation\/c3\/alert\/init()"},"doc://com.davidbjames.C3/documentation/C3/Alert/init(area:insets:gutter:options:)":{"role":"symbol","title":"init(area:insets:gutter:options:)","fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"("},{"kind":"externalParam","text":"area"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Layout","preciseIdentifier":"s:2C36LayoutV"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"Area","preciseIdentifier":"s:2C36LayoutV4AreaO"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"insets"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"InsetGroup","preciseIdentifier":"s:2C310InsetGroupV"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"gutter"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Size","preciseIdentifier":"s:2C34SizeV"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"options"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"AreaDefinitionOptions","preciseIdentifier":"s:2C321AreaDefinitionOptionsV"},{"kind":"text","text":")"}],"abstract":[{"type":"text","text":"Create a Guide that occupies a fixed area with optional insets."},{"type":"text","text":" "},{"type":"text","text":"For partial areas (e.g. .top) this guide can be used in conjunction"},{"type":"text","text":" "},{"type":"text","text":"with a remainder guide that occupies the remaining space."},{"type":"text","text":" "},{"type":"text","text":"In that case, the inset for the opposing edge will apply to"},{"type":"text","text":" "},{"type":"text","text":"the remainder rather than the current guide. Use “gutter”"},{"type":"text","text":" "},{"type":"text","text":"to add a gutter between this guide and the remainder guide."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Alert\/init(area:insets:gutter:options:)","kind":"symbol","type":"topic","url":"\/documentation\/c3\/alert\/init(area:insets:gutter:options:)"},"doc://com.davidbjames.C3/documentation/C3/Referential":{"role":"symbol","title":"Referential","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"Referential"}],"abstract":[{"type":"text","text":"Any item that can be used as a reference for positioning"},{"type":"text","text":" "},{"type":"text","text":"and sizing other items."},{"type":"text","text":" "},{"type":"text","text":"See also Queryable – the super-set of Referential."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Referential","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Referential"}],"url":"\/documentation\/c3\/referential"},"doc://com.davidbjames.C3/documentation/C3/PreDefinedArea":{"role":"symbol","title":"PreDefinedArea","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"PreDefinedArea"}],"abstract":[{"type":"text","text":"Flag protocol for "},{"type":"emphasis","inlineContent":[{"type":"text","text":"sub-classes"}]},{"type":"text","text":" of “container” types such as"},{"type":"text","text":" "},{"type":"text","text":"View, Layer, Guide, ScrollView, DisplayText, EditableText,"},{"type":"text","text":" "},{"type":"text","text":"TableView, CollectionView or ControlView."},{"type":"text","text":" "},{"type":"text","text":"Since these base classes provide automatic sizing (to fill"},{"type":"text","text":" "},{"type":"text","text":"the parent) you may want to conform to this protocol"},{"type":"text","text":" "},{"type":"text","text":"(PreDefinedArea) in order to prevent this automatic sizing"},{"type":"text","text":" "},{"type":"text","text":"from happening in case it "},{"type":"emphasis","inlineContent":[{"type":"text","text":"adversely"}]},{"type":"text","text":" alters the size of the item."},{"type":"text","text":" "},{"type":"text","text":"For example, the sizing may already be handled in"},{"type":"text","text":" "},{"type":"text","text":"initialization (e.g. init() method), or via custom"},{"type":"text","text":" "},{"type":"text","text":"construction (e.g. construct() method), or at the"},{"type":"text","text":" "},{"type":"text","text":"call-site as required (e.g. as with Grid)."},{"type":"text","text":" "},{"type":"text","text":"Conformance to this protocol (PreDefinedArea) may also provide"},{"type":"text","text":" "},{"type":"text","text":"a minor optimization by preventing "},{"type":"emphasis","inlineContent":[{"type":"text","text":"redundant"}]},{"type":"text","text":" sizing."},{"type":"text","text":" "},{"type":"text","text":"(For the curious, see View\/Layer\/Guide _construct() methods"},{"type":"text","text":" "},{"type":"text","text":"where the automatic sizing takes place which this protocol"},{"type":"text","text":" "},{"type":"text","text":"is used to prevent.)"}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/PreDefinedArea","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"PreDefinedArea"}],"url":"\/documentation\/c3\/predefinedarea"},"doc://com.davidbjames.C3/documentation/C3/Repeatable":{"role":"symbol","title":"Repeatable","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"Repeatable"}],"abstract":[{"type":"text","text":"Any item than can be repeatedly constructed using"},{"type":"text","text":" "},{"type":"text","text":"self as the base item."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Repeatable","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Repeatable"}],"url":"\/documentation\/c3\/repeatable"},"doc://com.davidbjames.C3/documentation/C3/TagSettable":{"role":"symbol","title":"TagSettable","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"TagSettable"}],"abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/TagSettable","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"TagSettable"}],"url":"\/documentation\/c3\/tagsettable"},"doc://com.davidbjames.C3/documentation/C3/LayerAssociable":{"role":"symbol","title":"LayerAssociable","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"LayerAssociable"}],"abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/LayerAssociable","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"LayerAssociable"}],"url":"\/documentation\/c3\/layerassociable"},"doc://com.davidbjames.C3/documentation/C3/Alert/init(coder:)":{"role":"symbol","title":"init(coder:)","fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"?("},{"kind":"externalParam","text":"coder"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"NSCoder","preciseIdentifier":"c:objc(cs)NSCoder"},{"kind":"text","text":")"}],"abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Alert\/init(coder:)","kind":"symbol","type":"topic","url":"\/documentation\/c3\/alert\/init(coder:)"},"doc://com.davidbjames.C3/documentation/C3/Dismissable":{"role":"symbol","title":"Dismissable","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"Dismissable"}],"abstract":[{"type":"text","text":"Protocol to support preparation and dismissal"},{"type":"text","text":" "},{"type":"text","text":"of a user interface object (class)."},{"type":"text","text":" "},{"type":"text","text":"Conformance to this protocol requires that the"},{"type":"text","text":" "},{"type":"text","text":"class be marked “final” due to Self requirement"},{"type":"text","text":" "},{"type":"text","text":"in the main method present()."},{"type":"text","text":" "},{"type":"text","text":"See also Presentable and PresentableWithData"},{"type":"text","text":" "},{"type":"text","text":"which are Dismissable."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Dismissable","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Dismissable"}],"url":"\/documentation\/c3\/dismissable"},"doc://com.davidbjames.C3/documentation/C3/Alert/construct(theme:debug:)":{"role":"symbol","title":"construct(theme:debug:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"construct"},{"kind":"text","text":"("},{"kind":"externalParam","text":"theme"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Theme","preciseIdentifier":"s:2C35ThemeV"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"debug"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"ApiDebug","preciseIdentifier":"s:2C38ApiDebugV"},{"kind":"text","text":"?)"}],"abstract":[{"type":"text","text":"Construct the current instance passing the appropriate"},{"type":"text","text":" "},{"type":"text","text":"theme to be used in construction. Alternatively, object"},{"type":"text","text":" "},{"type":"text","text":"can conform to ThemeAccessible to provide the theme."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Alert\/construct(theme:debug:)","kind":"symbol","type":"topic","url":"\/documentation\/c3\/alert\/construct(theme:debug:)"},"doc://com.davidbjames.C3/documentation/C3/QueryAccessible":{"role":"symbol","title":"QueryAccessible","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"QueryAccessible"}],"abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/QueryAccessible","kind":"sym