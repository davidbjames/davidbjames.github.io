LayoutV10QuadrantalO","extendedModule":"C3","modules":[{"name":"C3"}]},"hierarchy":{"paths":[["doc:\/\/com.davidbjames.C3\/documentation\/C3","doc:\/\/com.davidbjames.C3\/documentation\/C3\/Layout","doc:\/\/com.davidbjames.C3\/documentation\/C3\/Layout\/Quadrantal","doc:\/\/com.davidbjames.C3\/documentation\/C3\/Layout\/Quadrantal\/PathNode-Implementations"]]},"references":{"doc://com.davidbjames.C3/documentation/C3/Layout":{"role":"symbol","title":"Layout","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Layout"}],"abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Layout","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Layout"}],"url":"\/documentation\/c3\/layout"},"doc://com.davidbjames.C3/documentation/C3/Layout/Quadrantal/at(_:of:)":{"role":"symbol","title":"at(_:of:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"at"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"CGFloat","preciseIdentifier":"s:12CoreGraphics7CGFloatV"},{"kind":"text","text":", "},{"kind":"externalParam","text":"of"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Self"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Self"}],"abstract":[{"type":"text","text":"Given a relative\/absolute PathNode (e.g. Position, Quadrantal,"},{"type":"text","text":" "},{"type":"text","text":"CGPoint or BezierPoint) create a new one that is a % distance"},{"type":"text","text":" "},{"type":"text","text":"(expressed 0.0 to 1.0) between self and another."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Layout\/Quadrantal\/at(_:of:)","kind":"symbol","type":"topic","url":"\/documentation\/c3\/layout\/quadrantal\/at(_:of:)"},"doc://com.davidbjames.C3/documentation/C3/Layout/Quadrantal/PathNode-Implementations":{"role":"collectionGroup","title":"PathNode Implementations","abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Layout\/Quadrantal\/PathNode-Implementations","kind":"article","type":"topic","url":"\/documentation\/c3\/layout\/quadrantal\/pathnode-implementations"},"doc://com.davidbjames.C3/documentation/C3/Layout/Quadrantal":{"role":"symbol","title":"Layout.Quadrantal","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"Quadrantal"}],"abstract":[{"type":"text","text":"Quadrantal is a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"relative"}]},{"type":"text","text":" position unit based"},{"type":"text","text":" "},{"type":"text","text":"on decimal units between 0.0 and 1.0 for each"},{"type":"text","text":" "},{"type":"text","text":"axis, represented by 25 points on a rectangle"},{"type":"text","text":" "},{"type":"text","text":"with 5 columns and 5 rows. Use any of the 25"},{"type":"text","text":" "},{"type":"text","text":"convenience cases or array expressible syntax"},{"type":"text","text":" "},{"type":"text","text":"including offsets as applicable. As an alternative"},{"type":"text","text":" "},{"type":"text","text":"to offsets, use prefix\/postfix “shift” operators +\/-"},{"type":"text","text":" "},{"type":"text","text":"to further offset the point by 0.125 – effectively"},{"type":"text","text":" "},{"type":"text","text":"increasing the amount of relative points on the"},{"type":"text","text":" "},{"type":"text","text":"rectangle to 121. Use prefix +\/- to offset columns,"},{"type":"text","text":" "},{"type":"text","text":"and postfix +\/- to offset rows."},{"type":"text","text":" "},{"type":"text","text":"Use .point(on\/from) methods to convert a relative"},{"type":"text","text":" "},{"type":"text","text":"point to a real point (on a size, or from a rect)."},{"type":"text","text":" "},{"type":"text","text":"Example Quadrantals as a series of points for a path:"},{"type":"text","text":" "},{"type":"text","text":"[ .A1,   .D3 + [0.1,0],   [0.2,0.7],     -.C2,      .A5+ ]"},{"type":"text","text":" "},{"type":"text","text":"regular   with offset    custom point  col shift  row shift"},{"type":"text","text":" "},{"type":"text","text":"To specify points outside of the rectangle use"},{"type":"text","text":" "},{"type":"text","text":"column Z or F or rows 0 or 6 respectively, increasing"},{"type":"text","text":" "},{"type":"text","text":"the number of relative points on a rectangle to 225."},{"type":"text","text":" "},{"type":"text","text":"Advanced: Quadrantals also support specifying points"},{"type":"text","text":" "},{"type":"text","text":"with bezier control points to create cubic bezier curves."},{"type":"text","text":" "},{"type":"text","text":"Example: .C1 "},{"type":"emphasis","inlineContent":[{"type":"text","text":"– .C4  –"}]},{"type":"text","text":" .A5 creates a quadrantal C4 with"},{"type":"text","text":" "},{"type":"text","text":"two control points at C1 and A5 respectively."},{"type":"text","text":" "},{"type":"text","text":"If the second control point is nil it is the same as the first."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Layout\/Quadrantal","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Quadrantal"}],"url":"\/documentation\/c3\/layout\/quadrantal"},"doc://com.davidbjames.C3/documentation/C3":{"role":"collection","title":"C3","abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3","kind":"symbol","type":"topic","url":"\/documentation\/c3"}}}