"role":"symbol","title":"Background","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"Background"}],"abstract":[{"type":"text","text":"A layer representing a background."},{"type":"text","text":" "},{"type":"text","text":"Prefer using Background or BackgroundView over applying"},{"type":"text","text":" "},{"type":"text","text":"adhoc background changes to container views."},{"type":"text","text":" "},{"type":"text","text":"See also SnapshotBackground for use in lists."},{"type":"text","text":" "},{"type":"text","text":"Backgrounds are generally the “first sibling” to whatever"},{"type":"text","text":" "},{"type":"text","text":"it is providing a background for and would rarely have"},{"type":"text","text":" "},{"type":"text","text":"children (except for custom backgrounds)."},{"type":"text","text":" "},{"type":"text","text":"Backgrounds have a specific construction, for example:"},{"type":"text","text":" "},{"type":"text","text":"Where parent is a layer, the background is sent to back,"},{"type":"text","text":" "},{"type":"text","text":"it is stretched to fit it’s parent (or including “outset”"},{"type":"text","text":" "},{"type":"text","text":"if specified on creation), it masks content to it’s bounds"},{"type":"text","text":" "},{"type":"text","text":"(unless overridden) and it inherits ancestor corner radius."},{"type":"text","text":" "},{"type":"text","text":"When Background is used in conjunction with “in()”,"},{"type":"text","text":" "},{"type":"text","text":"a focussed API is provided to background mutations,"},{"type":"text","text":" "},{"type":"text","text":"include full Theme support."},{"type":"text","text":" "},{"type":"text","text":"For more complex Backgrounds, including multilayer"},{"type":"text","text":" "},{"type":"text","text":"Backgrounds and layering effects, use make{} or build{}"},{"type":"text","text":" "},{"type":"text","text":"on the Background instance to add hierarchy,"},{"type":"text","text":" "},{"type":"text","text":"or use the convenience API add{} and addLayer{}."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Background","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Background"}],"url":"\/documentation\/c3\/background"},"doc://com.davidbjames.C3/documentation/C3":{"role":"collection","title":"C3","abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3","kind":"symbol","type":"topic","url":"\/documentation\/c3"},"doc://com.davidbjames.C3/documentation/C3/Background/clone(from:cheaply:)":{"role":"symbol","title":"clone(from:cheaply:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"clone"},{"kind":"text","text":"("},{"kind":"externalParam","text":"from"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"AnyObject","preciseIdentifier":"s:s9AnyObjecta"},{"kind":"text","text":", "},{"kind":"externalParam","text":"cheaply"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"},{"kind":"text","text":")"}],"abstract":[],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Background\/clone(from:cheaply:)","kind":"symbol","type":"topic","url":"\/documentation\/c3\/background\/clone(from:cheaply:)"}}}