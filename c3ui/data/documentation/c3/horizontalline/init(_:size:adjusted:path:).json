vidbjames.C3\/documentation\/C3\/HorizontalLine\/init(_:size:adjusted:path:)","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Create line with series of relative points represented"},{"type":"text","text":" "},{"type":"text","text":"as Quadrantals which support both straight lines and"},{"type":"text","text":" "},{"type":"text","text":"curved lines via bezier curves."},{"type":"text","text":" "},{"type":"text","text":"Example “swish” line from left to right:"},{"type":"text","text":" "},{"type":"text","text":"[.A4 –* +.A2-, -.C2- "},{"type":"emphasis","inlineContent":[{"type":"text","text":"– .C3 =="}]},{"type":"text","text":" 1.0, -.E4+ *– .E2]"}],"kind":"symbol","metadata":{"fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"(["},{"kind":"typeIdentifier","text":"Layout","preciseIdentifier":"s:2C36LayoutV"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"Quadrantal","preciseIdentifier":"s:2C36LayoutV10QuadrantalO"},{"kind":"text","text":"], "},{"kind":"externalParam","text":"size"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Size","preciseIdentifier":"s:2C34SizeV"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"adjusted"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Size","preciseIdentifier":"s:2C34SizeV"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"path"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Shape","preciseIdentifier":"c:@M@C3@objc(cs)Shape"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"PathSync","preciseIdentifier":"s:2C35ShapeC8PathSyncO"},{"kind":"text","text":")"}],"title":"init(_:size:adjusted:path:)","roleHeading":"Initializer","role":"symbol","symbolKind":"init","externalID":"s:2C314HorizontalLineC_4size8adjusted4pathACSayAA6LayoutV10QuadrantalOG_AA4SizeVSgAnA5ShapeC8PathSyncOtcfc","modules":[{"name":"C3"}]},"hierarchy":{"paths":[["doc:\/\/com.davidbjames.C3\/documentation\/C3","doc:\/\/com.davidbjames.C3\/documentation\/C3\/HorizontalLine"]]},"references":{"doc://com.davidbjames.C3/documentation/C3/Layout/Quadrantal":{"role":"symbol","title":"Layout.Quadrantal","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"Quadrantal"}],"abstract":[{"type":"text","text":"Quadrantal is a "},{"type":"emphasis","inlineContent":[{"type":"text","text":"relative"}]},{"type":"text","text":" position unit based"},{"type":"text","text":" "},{"type":"text","text":"on decimal units between 0.0 and 1.0 for each"},{"type":"text","text":" "},{"type":"text","text":"axis, represented by 25 points on a rectangle"},{"type":"text","text":" "},{"type":"text","text":"with 5 columns and 5 rows. Use any of the 25"},{"type":"text","text":" "},{"type":"text","text":"convenience cases or array expressible syntax"},{"type":"text","text":" "},{"type":"text","text":"including offsets as applicable. As an alternative"},{"type":"text","text":" "},{"type":"text","text":"to offsets, use prefix\/postfix “shift” operators +\/-"},{"type":"text","text":" "},{"type":"text","text":"to further offset the point by 0.125 – effectively"},{"type":"text","text":" "},{"type":"text","text":"increasing the amount of relative points on the"},{"type":"text","text":" "},{"type":"text","text":"rectangle to 121. Use prefix +\/- to offset columns,"},{"type":"text","text":" "},{"type":"text","text":"and postfix +\/- to offset rows."},{"type":"text","text":" "},{"type":"text","text":"Use .point(on\/from) methods to convert a relative"},{"type":"text","text":" "},{"type":"text","text":"point to a real point (on a size, or from a rect)."},{"type":"text","text":" "},{"type":"text","text":"Example Quadrantals as a series of points for a path:"},{"type":"text","text":" "},{"type":"text","text":"[ .A1,   .D3 + [0.1,0],   [0.2,0.7],     -.C2,      .A5+ ]"},{"type":"text","text":" "},{"type":"text","text":"regular   with offset    custom point  col shift  row shift"},{"type":"text","text":" "},{"type":"text","text":"To specify points outside of the rectangle use"},{"type":"text","text":" "},{"type":"text","text":"column Z or F or rows 0 or 6 respectively, increasing"},{"type":"text","text":" "},{"type":"text","text":"the number of relative points on a rectangle to 225."},{"type":"text","text":" "},{"type":"text","text":"Advanced: Quadrantals also support specifying points"},{"type":"text","text":" "},{"type":"text","text":"with bezier control points to create cubic bezier curves."},{"type":"text","text":" "},{"type":"text","text":"Example: .C1 "},{"type":"emphasis","inlineContent":[{"type":"text","text":"– .C4  –"}]},{"type":"text","text":" .A5 creates a quadrantal C4 with"},{"type":"text","text":" "},{"type":"text","text":"two control points at C1 and A5 respectively."},{"type":"text","text":" "},{"type":"text","text":"If the second control point is nil it is the same as the first."}],"identifier":"doc:\/\/com.davidbjames.C3\/documentation\/C3\/Layout\/Quadrantal","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Quadrantal"}],"url":"\/documentation\/c3\/layout\/quadrantal"},"doc://com.davidbjames.C3/documentation/C3/HorizontalLine/init(_:size:adjusted:path:)":{"role":"symbol","title":"init(_:size:adjusted:path:)","fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"(["},{"kind":"typeIdentifier","text":"Layout","preciseIdentifier":"s:2C36LayoutV"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"Quadrantal","preciseIdentifier":"s:2C36LayoutV10QuadrantalO"},{"kind":"text","text":"], "},{"kind":"externalParam","text":"size"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Size","preciseIdentifier":"s:2C34SizeV"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"adjusted"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Size","preciseIdentifier":"s:2C34SizeV"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"path"},{"kind":"text","text":": "},{"kind":