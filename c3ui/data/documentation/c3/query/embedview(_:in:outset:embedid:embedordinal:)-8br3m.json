":"text","text":" "},{"type":"text","text":"item(s) to be reusable."},{"type":"text","text":" "},{"type":"text","text":"IMPORTANT: when embedding multiple reusable items be sure to use"},{"type":"text","text":" "},{"type":"text","text":"the same set of items (in the same order) if calling this again"},{"type":"text","text":" "},{"type":"text","text":"so that they are ordinalized correctly wrt reusable attachment."},{"type":"text","text":" "},{"type":"text","text":"If embedding in a Live layout you will need to pass builder option"},{"type":"text","text":" "},{"type":"codeVoice","code":"embedded"},{"type":"text","text":" on attachment of the item(s) and ensure the embed is"},{"type":"text","text":" "},{"type":"text","text":"reusable so that the hierarchy will not be redundantly constructed."},{"type":"text","text":" "},{"type":"text","text":"Example:"}],"kind":"symbol","metadata":{"conformance":{"constraints":[{"type":"codeVoice","code":"B"},{"type":"text","text":" conforms to "},{"type":"codeVoice","code":"Queryable"},{"type":"text","text":" and "},{"type":"codeVoice","code":"T"},{"type":"text","text":" inherits "},{"type":"codeVoice","code":"CALayer"},{"type":"text","text":"."}],"availabilityPrefix":[{"type":"text","text":"Available when"}],"conformancePrefix":[{"type":"text","text":"Conforms when"}]},"role":"symbol","title":"embedView(_:in:outset:embedId:embedOrdinal:)","roleHeading":"Instance Method","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"embedView"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"E"},{"kind":"text","text":">("},{"kind":"typeIdentifier","text":"E","preciseIdentifier":"s:2C35QueryVAASo7CALayerCRb_rlE9embedView_2in6outset0D2Id0D7OrdinalACyxqd__Gqd__mSg_AA17AutoConstrainable_AA9QueryablepSgAA10InsetGroupVSgSSSgSiSgtAA05EmbedE0CRbd__lF1EL_qd__mfp"},{"kind":"text","text":".Type?, "},{"kind":"externalParam","text":"in"},{"kind":"text","text":": ("},{"kind":"typeIdentifier","te